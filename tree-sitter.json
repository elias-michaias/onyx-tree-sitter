{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "onyx",
      "camelcase": "Onyx",
      "title": "Onyx",
      "scope": "source.onyx",
      "file-types": [
        "onyx"
      ],
      "injection-regex": "^onyx$",
      "class-name": "TreeSitterOnyx"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Grammar for the Onyx programming language.",
    "authors": [
      {
        "name": "Elias Michaias",
        "email": "emskeirik@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/elias-michaias/onyx-tree-siter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}